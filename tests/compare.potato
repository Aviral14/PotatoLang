int main() {
    int n1; 
    int n2; 
    int n3;
    n1 = 23;
    n2 = 32;
    nx = 40;
    n3 = (nx / (n2 * n1));

    /* if n1 is greater than both n2 and n3, n1 is the largest */
    if (((n1 >= n2) && (n1 >= n3))){
        println(n1);
    }

    /* if n2 is greater than both n1 and n3, n2 is the largest */
    if (((n2 >= n1) && (n2 >= n3))){
        println(n2);
    }

    /* if n3 is greater than both n1 and n3, n3 is the largest */
    if (((n3 >= n1) || (n3 >= n2))){
        println(n3);
    }
}
